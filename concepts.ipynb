   "source": [
    "# Use zip to iterate over multiple lists at the same time\n",
    "\n",
    "names = [\"a\", \"b\"]\n",
    "scores = [10, 20]\n",
    "\n",
    "for n, s in zip(names, scores):\n",
    "    print(n, s)\n"
   ]
